<tool id="Range" name="Range" version="1.0">
  <description>Create a range of values until int similar to python range(x).</description>
  <authors>
    <author name="Hakim Achterberg" email="h.achterberg@erasmusmc.nl" url="http://www.bigr.nl/people/HakimAchterberg" />
  </authors>
  <command version="1.0" url="">
    <targets>
      <target os="*" arch="*" interpreter="python" paths="./bin" bin="range.py" />
    </targets>
    <description>
        python range.py --in 5
        output = input
    </description>
    <authors>
      <author name="Hakim Achterberg" email="h.achterberg@erasmusmc.nl" url="http://www.bigr.nl/people/HakimAchterberg" />
    </authors>
  </command>
  <repository />
  <interface>
      <inputs>
        <input id="value" name="Input integer" datatype="Int" prefix="--in" cardinality="1" required="true" />
      </inputs>
      <outputs>
        <output id="result" name="Output image file" datatype="Int" automatic="True" cardinality="unknown" method="json" location="^RESULT_1=(.*)$"/>
      </outputs>
  </interface>
  <requirements />
  <tests />
  <help>
    RTFM! =]
  </help>
  <cite>
  </cite>
  <references />
</tool>
