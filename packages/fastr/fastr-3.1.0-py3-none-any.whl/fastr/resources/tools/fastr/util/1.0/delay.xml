<tool id="Delay" name="Delay before copying the input to the output." version="1.0">
  <description>Delay before copying the input and the output.</description>
  <authors>
    <author name="Marcel Koek" email="m.koek@erasmusmc.nl" url="http://www.bigr.nl/people/MarcelKoek" />
  </authors>
  <command version="1.0" url="">
    <targets>
      <target os="*" arch="*" interpreter="python" paths="./bin" bin="delay.py" />
    </targets>
    <description>
        python delay.py --in filename --out filename
        output = input
    </description>
    <authors>
      <author name="Marcel Koek" email="m.koek@erasmusmc.nl" url="http://www.bigr.nl/people/MarcelKoek" />
    </authors>
  </command>
  <repository />
  <interface>
      <inputs>
        <input id="image" name="Input image file" datatype="ITKImageFile" prefix="--in" cardinality="1" repeat_prefix="false" required="true" />
      </inputs>
      <outputs>
        <output id="image" name="Output image file" datatype="ITKImageFile" prefix="--out" cardinality="1">
            <description>A copy of the input.</description>
        </output>
      </outputs>
  </interface>
  <requirements />
  <tests />
  <help>
    RTFM! =]
  </help>
  <cite>
  </cite>
  <references />
</tool>
