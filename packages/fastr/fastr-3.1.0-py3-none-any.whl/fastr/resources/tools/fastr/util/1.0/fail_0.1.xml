<tool id="Fail" name="Delay before copying the input to the output." version="0.1">
  <description>Delay before copying the input and the output.</description>
  <authors>
    <author name="Adriaan Versteeg" email="a.bersteeg@erasmusmc.nl" url="http://www.bigr.nl/people/AdriaanVersteeg" />
  </authors>
  <command version="1.0" url="">
    <targets>
      <target os="*" arch="*" interpreter="python" paths="./bin" bin="fail.py" />
    </targets>
    <description>
        python fail.py --in1 fake_input --in2 --out1 filename --out2 fake output --fail1 --fail2
        output = input
    </description>
    <authors>
      <author name="Adriaan Versteeg" email="a.bersteeg@erasmusmc.nl" url="http://www.bigr.nl/people/AdriaanVersteeg" />
    </authors>
  </command>
  <repository />
  <interface>
      <inputs>
        <input id="in_1" name="Input image file" datatype="Int" prefix="--in_1" cardinality="1" repeat_prefix="false" required="true" />
        <input id="in_2" name="Input image file" datatype="Int" prefix="--in_2" cardinality="1" repeat_prefix="false" required="true" />
        <input id="fail_1" name="Input image file" datatype="Boolean" prefix="--fail_1" format="flag" required="false" repeat_prefix="false" />
        <input id="fail_2" name="Input image file" datatype="Boolean" prefix="--fail_2" format="flag" required="false" repeat_prefix="false" />
      </inputs>
      <outputs>
        <output id="out_1" name="Output image file" datatype="Int" automatic="True" cardinality="1" method="json" location="^RESULT_1=(.*)$"/>
        <output id="out_2" name="Output image file" datatype="Int" automatic="True" cardinality="1" method="json" location="^RESULT_2=(.*)$"/>
      </outputs>
  </interface>
  <requirements />
  <tests />
  <help>
    RTFM! =]
  </help>
  <cite>
  </cite>
  <references />
</tool>
