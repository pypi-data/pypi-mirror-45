#!/home/fox/stow/packages/miniconda3/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pycapo==0.2.1','console_scripts','pycapo'
__requires__ = 'pycapo==0.2.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pycapo==0.2.1', 'console_scripts', 'pycapo')()
    )
