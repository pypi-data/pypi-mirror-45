#NEXUS
BEGIN TAXA;
  DIMENSIONS NTAX=4;
  TAXLABELS fish frog snake mouse;
END;
BEGIN CHARACTERS;
  DIMENSIONS NCHAR=20;
  FORMAT DATATYPE=DNA;
  MATRIX
    fish  ACATA GAGGG TACCT CTAAG
    frog  ACTTA GAGGC TACCT CTACG
    snake ACTCA CTGGG TACCT TTGCG
    mouse ACTCA GACGG TACCT TTGCG;
END;

BEGIN TREES;
  TREE best=(fish, (frog, (snake, mouse)));
END;