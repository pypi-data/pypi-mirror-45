#: validators.py:142
#, python-format
msgid ""
"This password is too short. It must contain at least %(min_length)d "
"character."
msgid_plural ""
"This password is too short. It must contain at least %(min_length)d "
"characters."
msgstr[0] ""
"Ce mot de passe est trop court. Il doit contenir au moins  %(min_length)d "
"caractère."
msgstr[1] ""
"Ce mot de passe est trop court. Il doit contenir au moins  %(min_length)d "
"caractères."

#: validators.py:152
#, python-format
msgid "Your password must contain at least %(min_length)d character."
msgid_plural "Your password must contain at least %(min_length)d characters."
msgstr[0] ""
"Votre mot de passe  doit contenir au moins  %(min_length)d caractère."
msgstr[1] ""
"Votre mot de passe  doit contenir au moins  %(min_length)d caractères."

#: validators.py:189
#, python-format
msgid "The password is too similar to the %(verbose_name)s."
msgstr "Le mot de passe est trop similaire à: %(verbose_name)s."

#: validators.py:195
msgid "Your password can't be too similar to your other personal information."
msgstr ""
"Votre mot de passe ne doit pas être trop similaire aux autres champs "
"d'information personnelle."

#: validators.py:222
msgid "This password is too common."
msgstr "Le mot de passe est trop commun."

#: validators.py:227
msgid "Your password can't be a commonly used password."
msgstr "Votre mot de passe ne doit pas être trop commun."

#: validators.py:237
msgid "This password is entirely numeric."
msgstr "Le mot de passe est entièrement numérique."

#: validators.py:242
msgid "Your password can't be entirely numeric."
msgstr "Votre mot de passe ne doit pas être entièrement numérique."

#: validators.py:252
msgid "This password does not contain a digit."
msgstr "Le mot de passe ne contient pas de chiffre."

#: validators.py:257
msgid "Your password must contain at least one digit."
msgstr "Votre mot de passe doit contenir au moins un chiffre."

#: validators.py:267
msgid "This password does not contain a punctuation character."
msgstr "Le mot de passe ne contient pas de signe de ponctuation."

#: validators.py:272
msgid "Your password must contain at least one punctuation character."
msgstr "Votre mot de passe doit contenir au moins un signe de ponctuation."

#: validators.py:282
msgid "This password does not contain an uppercase character."
msgstr "Le mot de passe ne contient pas de caractère en majuscule."

#: validators.py:287
msgid "Your password must contain at least one uppercase character."
msgstr "Votre mot de passe doit contenir au moins un caractère en majuscule."

#: validators.py:297
msgid "This password does not contain a lowercase character."
msgstr "Le mot de passe ne contient pas de caractère en minuscule."

#: validators.py:302
msgid "Your password must contain at least one lowercase character."
msgstr "Votre mot de passe doit contenir au moins un caractère en minuscule."

#: validators.py:317
#, python-format
msgid ""
"This password contains a character repeated more than %d times in a row."
msgstr "Le mot de passe contient plus de %d caracteres répétés à la suite."

#: validators.py:322
#, python-format
msgid ""
"Your password can't contain a character repeated more than %d times in a row."
msgstr ""
"Votre mot de passe ne doit pas contenir plus de %d caracteres répétés à la "
"suite."
