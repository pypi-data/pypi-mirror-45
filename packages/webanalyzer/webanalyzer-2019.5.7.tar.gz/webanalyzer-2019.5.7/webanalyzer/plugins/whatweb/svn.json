{
    "name": "SVN",
    "author": "Brendan Coles <bcoles@gmail.com>",
    "version": "0.2",
    "description": "SVN HTTP header.",
    "website": null,
    "matches": [
        {
            "regexp": "(?-mix:[^\\r^\\n]*SVN)",
            "search": "headers[server]"
        },
        {
            "search": "headers[server]",
            "regexp": "(?-mix:[^\\r^\\n]*SVN\\/([^\\r^\\n^\\s]*))",
            "offset": 1
        }
    ]
}
