{
    "name": "MetaGenerator",
    "author": "Andrew Horton",
    "version": "0.3",
    "description": "This plugin identifies meta generator tags and extracts its value.",
    "website": null,
    "matches": [
        {
            "regexp": "(?i-mx:<meta[^>^=]+content[\\s]*=[\\s]*[\"|']?([^\"^'^>]+)[\"|']?[^>^=]+name[\\s]*=[\\s]*[\"|']?generator[\"|']?)"
        },
        {
            "regexp": "(?i-mx:<meta[^>^=]+name[\\s]*=[\\s]*[\"|']?generator[\"|']?[^>^=]+content[\\s]*=[\\s]*\"([^\"^'^>]+)\")"
        }
    ]
}
