{
    "name": "NetBox",
    "author": "Brendan Coles <bcoles@gmail.com>",
    "version": "0.2",
    "description": "NetBox HTTP server [Chinese]",
    "website": "http://www.netbox.cn/",
    "matches": [
        {
            "regexp": "(?-mix:^NetBox)",
            "search": "headers[server]"
        },
        {
            "search": "headers[server]",
            "regexp": "(?-mix:^NetBox Version (.+)$)",
            "offset": 1
        }
    ]
}
