{
    "name": "RDoc",
    "website": "https://github.com/RDoc/RDoc",
    "matches": [
        {
            "regexp": "<link[^>]+href=\"[^\"]*rdoc-style\\.css"
        },
        {
            "regexp": "Generated by <a[^>]+href=\"https?://rdoc\\.rubyforge\\.org[^>]+>RDoc</a> ([\\d.]*\\d)",
            "offset": 1
        }
    ],
    "implies": "Ruby"
}