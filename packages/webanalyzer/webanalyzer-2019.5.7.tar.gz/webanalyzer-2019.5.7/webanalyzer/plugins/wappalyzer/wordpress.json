{
    "name": "WordPress",
    "website": "https://wordpress.org",
    "matches": [
        {
            "regexp": "<link rel=[\"']stylesheet[\"'] [^>]+/wp-(?:content|includes)/"
        },
        {
            "regexp": "<div[^>]*class=[\"']amp-wp-"
        },
        {
            "regexp": "<link[^>]+s\\d+\\.wp\\.com"
        },
        {
            "search": "headers[link]",
            "regexp": "rel=\"https://api\\.w\\.org/\""
        },
        {
            "search": "meta[generator]",
            "regexp": "^WordPress ?([\\d.]+)?",
            "offset": 1
        },
        {
            "search": "script",
            "regexp": "/wp-(?:content|includes)/"
        }
    ],
    "implies": [
        "PHP",
        "MySQL"
    ]
}