{
    "name": "Font Awesome",
    "website": "http://fontawesome.io",
    "matches": [
        {
            "regexp": "<link[^>]* href=[^>]+(?:([\\d.]+)/)?(?:css/)?font-awesome(?:\\.min)?\\.css",
            "offset": 1
        },
        {
            "regexp": "<script[^>]* src=[^>]+fontawesome(?:\\.js)?"
        }
    ]
}