{
    "name": "AngularJS",
    "website": "https://angularjs.org",
    "matches": [
        {
            "regexp": "<(?:div|html)[^>]+ng-app="
        },
        {
            "regexp": "<ng-app"
        },
        {
            "search": "script",
            "regexp": "angular[.-]([\\d.]*\\d)[^/]*\\.js",
            "offset": 1
        },
        {
            "search": "script",
            "regexp": "/([\\d.]+(?:-?rc[.\\d]*)*)/angular(?:\\.min)?\\.js",
            "offset": 1
        },
        {
            "search": "script",
            "regexp": "angular.*\\.js"
        }
    ],
    "excludes": [
        "Angular",
        "AngularDart"
    ]
}