[run]
branch = True
source = miRTarBase
omit =
    src/bio2bel_mirtarbase/__main__.py
    src/bio2bel_mirtarbase/cli.py
    tests/*

[paths]
source =
    src/bio2bel_mirtarbase
    .tox/*/lib/python*/site-packages/bio2bel_mirtarbase

[report]
show_missing = True
exclude_lines =
    def __str__
    def __repr__
