#!/usr/bin/python3

import os, sys ; sys.path.insert(0, os.getcwd())

from bot import cfg, __version__
from bot.run import kernel
from obj.shell import reset, startup

startup()
try:
    k = kernel.start("botlib", __version__)
    k.walk("bot")
    k.wait()
except KeyboardInterrupt:
    print("")
reset()
os._exit(0)
